<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fi.metropolia.easytocook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EasyToCook">
        <activity android:name=".search_by_time"></activity>
        <activity android:name=".search_by_ingredients" />
        <activity android:name=".Main_menu" />
        <activity android:name=".Activity_ingredients" />
        <activity android:name=".signup_login" />
        <activity android:name=".Activity_userProfile" />
        <activity
            android:name=".Activity_logIn"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>